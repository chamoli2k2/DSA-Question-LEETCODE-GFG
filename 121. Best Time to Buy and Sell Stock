class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int profit=0;
        int mini=INT_MAX;
        int ans=INT_MIN;
        int n=prices.size();
        for(int i=0;i<n;i++){
            int temp=prices[i];
            if(mini>temp){
                mini=temp;
            }
            profit=prices[i]-mini;
            if(profit>ans){
                ans=profit;
            }
        }
        return ans;
    }
};
