class Solution{
  public:
    
    // A[]: input array
    // N: input array
    //Function to sort the binary array.
    void binSort(int arr[], int N)
    {
        int k = 0;
       for(int i=0;i<N;i++){
           if(arr[i] == 0){
               swap(arr[i],arr[k]);
               k++;
           }
       }
    }
};
