class Solution {
private:
    int solve(string &s,int i,int j){
        // Base case
        if(i == j) return 1;

        if(dp[i][j] != -1) return dp[i][j];

        int ans = INT_MAX;
        for(int k=i;k<j;k++){
            int temp = solve(s,i,k) + solve(s,k+1,j);
            ans = min(ans,temp);
        }

        if(s[i] == s[j]) ans--;
        
        return dp[i][j] = ans;
    }
public:
    int dp[101][101];
    int strangePrinter(string s){
        memset(dp,-1,sizeof(dp));
        return solve(s,0,s.size()-1);
    }
};
