class Solution {
public:
    int findMaxK(vector<int>& nums) {
        unordered_map<int,int> mp;
        for(auto i : nums){
            mp[i]++;
        }
        
        sort(nums.begin(),nums.end());
        int ans = -1;
        for(int i=nums.size()-1;i>=1;i--){
            if(mp.find(-nums[i]) != mp.end()){
                ans = nums[i];
                break;
            }
        }
        
        return ans;
    }
};
