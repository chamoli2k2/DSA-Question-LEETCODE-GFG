class Solution{
public:
	void merge(int arr1[], int arr2[], int n, int m) {
	    // Logic is simple , first we try every element is in right position of it
	    for(int i=n-1,j=0;i>=0 and j<m;i--,j++){
	        // checking if last1 element of arr is smaller than first element of arr1
	        if(arr1[i] > arr2[j]){
	            swap(arr1[i],arr2[j]);
	        }
	    }
	    
	    sort(arr1,arr1+n);
	    sort(arr2,arr2+m);
	    
	}
};
