class Solution {
public:
    int numTrees(int N) {
        // concept is of catlon number
        if(N < 2){
            return 1;
        }
        
        // creating dp array
        vector<long long> dp(N+1,0);
        dp[0] = 1;
        dp[1] = 1;
        int mod = 1e9 + 7;
        for(int i=2;i<=N;i++){
            long long res = 0;
            for(int j=0;j<i;j++){
                long long left = dp[j];
                long long right = dp[i-j-1];
                
                res += (left * right);
            }
            
            dp[i] = res;
        }
        
        return dp[N];
    }
};
