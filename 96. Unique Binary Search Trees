class Solution {
private:
    int solve(int n,vector<int> &dp){
        // Base case
        if(n < 2){
            return 1;
        }

        if(dp[n] != -1){
            return dp[n];
        }

        int ans = 0;
        for(int i=1;i<=n;i++){
            ans += solve(i-1,dp) * solve(n-i,dp);
        }

        return dp[n] = ans;
    }
public:
    int numTrees(int n) {
        vector<int> dp(n+1,-1);
        return solve(n,dp);
    }
};
