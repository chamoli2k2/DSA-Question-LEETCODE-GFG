#include <bits/stdc++.h>
using namespace std;
int solve(vector<int> &nums){
    int n = nums.size();
    
    int ans = INT_MIN;
    for(int i=0; i<n; i+=2){
        ans = max(ans, nums[i]);
    }
    
    return ans;
}
int main()
{
    int t;
    cin>>t;
    
    while(t--){
        int n;
        cin>>n;
        vector<int> nums(n);
        for(int i=0; i<n; i++){
            int k;
            cin>>k;
            nums[i] = k;
        }
        
        int ans = solve(nums);
        cout<<ans<<endl;
    }

    return 0;
}