#include <bits/stdc++.h>
using namespace std;
string solve(string &s){
    int n = s.size();
    string ans = s;
    
    int flag = 0;
    for(int i=1; i<n; i++){
        if(s[i] == s[i-1]){
            flag = 1;
            if(s[i] == 'z'){
                ans.insert(ans.begin()+i, 'a');
            }
            else{
                ans.insert(ans.begin()+i, (s[i-1]+1));
            }
            break;
        }
    }
    
    if(flag){
        return ans;
    }
    
    if(s[n-1] == 'z'){
        ans += 'a';
    }
    else{
        ans += (s[n-1]+1);
    }
    
    return ans;
}
 
 
int main()
{
    int t;
    cin>>t;
    
    while(t--){
        string s;
        cin>>s;
        
        string ans = solve(s);
        cout<<ans<<endl;
    }
 
    return 0;
}